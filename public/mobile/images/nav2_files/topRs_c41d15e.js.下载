define("searchresult:widget/pagelets/base/topRs/topRs",function(e){function t(e){this.$container=null,this.option=e||{},this.hasClick=!1,this.changeFilter=!1}var i=e("common:widget/ui/base/base"),a=e("common:widget/ui/utils/utils");return i.extend(t.prototype,{init:function(e){this.$container=i("#topRS"),this.changeFilter=e.changeFilter,this.bindEvents()},bindEvents:function(){var e=this;this.$container.delegate("a","click",function(t){if(t.preventDefault(),!e.hasClick){var n={},l=i(this).attr("href");a.selectedFilter().isSelected&&e.changeFilter&&(n={lm:a.getQueryValue("lm")||-1,z:a.getQueryValue("z")||0,ic:a.getQueryValue("ic")||0,width:a.getQueryValue("width")||0,height:a.getQueryValue("height")||0,latest:a.getQueryValue("latest")||0,hd:a.getQueryValue("hd")||0,copyright:a.getQueryValue("copyright")||0});var r=a.modifyUrl(n,l);e.hasClick=!0,location.href=r}})}}),t});